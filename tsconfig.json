{
  "compilerOptions": {
    "noEmit": true,
    "checkJs": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true,
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "NodeNext",
    "verbatimModuleSyntax": true
  }
}
